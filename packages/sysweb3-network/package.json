{
  "name": "@pollum-io/sysweb3-network",
  "version": "1.0.91",
  "description": "A network management tool for multi-chain accounts.",
  "main": "esm/index.js",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pollum-io/sysweb3.git"
  },
  "author": "Pali Wallet",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/Pollum-io/sysweb3/issues"
  },
  "homepage": "https://github.com/Pollum-io/sysweb3#readme",
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "tsc && tsc -p tsconfig.json",
    "rollup": "npx rollup -c ../../scripts/config/rollup.config.js",
    "prerelease": "yarn build && yarn version patch",
    "copy": "node ../../scripts/package/copy",
    "release:npm": "npm publish ./dist --access=public",
    "release:preflight": "yarn pack ./dist --dry-run",
    "publish": "yarn prebuild && yarn build && yarn rollup && yarn prerelease && yarn copy && yarn release:npm",
    "type-check": "tsc --pretty --noEmit",
    "localTest": "yarn prebuild && yarn build && yarn rollup && yarn build && yarn copy"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bip44-constants": "^114.0.0",
    "bs58check": "^2.1.2",
    "eth-chains": "^1.0.0",
    "ethers": "^5.6.9",
    "web3": "^1.7.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191"
  }
}
